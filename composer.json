{
  "name": "symfonylab/doctrine-orm-point-bundle",
  "type": "symfony-bundle",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "symfony/framework-bundle": "^4.3",
    "doctrine/doctrine-bundle": "^1.9",
    "doctrine/orm": "^2.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.2"
  },
  "autoload": {
    "psr-4": {
      "SymfonyLab\\DoctrineOrmPoint\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "SymfonyLab\\DoctrineOrmPoint\\Tests\\": "Tests/"
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
